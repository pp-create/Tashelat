                    $("#phonecontect11").html(res.phone);
                    $("#twitter1").attr("href", res.twiter);
                    $("#facebook1").attr("href", res.facebock);
                    $("#instagram1").attr("href", res.tiktok);
                    $("#linkedin1").attr("href", res.linkedin);
                    $("#twitter").attr("href", res.twiter);
                    $("#facebook").attr("href", res.facebock);
