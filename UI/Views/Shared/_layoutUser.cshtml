        function Getdataservicesitem() {
            $.ajax({
                type: "GET",
                url: "/Viewcustomer/side",
                success: function (res) {
                   
                    var html = "";

                    // Assuming `res` is an array of objects
                    $.each(res, function (i, e) {
                        // Use `e` to access each item in the `res` array
                        html +=`<li><a href="/Viewcustomer/viewservice?sidename=${e.sidename}&contect=${e.contect}">${e.sidename}</a></li>
                            
                        `;
                    });

                    // Update the HTML content of the element with id 'servicesdata'
                    $("#servicesdata1111111111").html(html); $("#dataservicesss").html(html);
                },
                error: function (xhr, status, error) {
                    console.error("An error occurred: " + error);
                }
            });
        }

        function getcontect() {
